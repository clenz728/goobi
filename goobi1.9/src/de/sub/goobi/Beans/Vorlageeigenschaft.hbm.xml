<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="de.sub.goobi.Beans.Vorlageeigenschaft" table="vorlageneigenschaften">
		<id name="id" column="vorlageneigenschaftenID" type="integer">
			<generator class="native" />
		</id>

		<property name="titel" column="Titel" type="string" />
		<property name="wert" column="Wert" type="string" />
		<property name="istObligatorisch" column="IstObligatorisch" />
		<property name="datentyp" column="DatentypenID" type="integer" />
		<property name="auswahl" column="Auswahl" type="string" />
		<property name="creationDate" column="creationDate" type="timestamp"/>
		<property name="container" column="container" type="integer" />			
		<many-to-one name="vorlage" class="de.sub.goobi.Beans.Vorlage" column="vorlagenID"/>
		
	</class>

</hibernate-mapping>
